/*           Animations              */
.animation-transition-general, .sidebar .nav p, .sidebar .user .photo,
.bootstrap-navbar .user .photo, .sidebar .user a,
.bootstrap-navbar .user a, .login-page .card {
  -webkit-transition: all 300ms linear;
